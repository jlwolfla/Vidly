@model IEnumerable<Vidly.Models.Movie>

@{
    ViewBag.Title = "Movies";
}

<h2>Movies</h2>


<table class="table table-striped">
    <tr>
        <th>
            Movie
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink(item.Name, "Details", new { id = item.Id })
        </td>

    </tr>
}

</table>
